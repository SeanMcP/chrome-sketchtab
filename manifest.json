{
	"name": "Sketchtab",
	"short_name": "Sketchtab",
	"description": "New tab page with code editor",
	"version": "0.6",
	"incognito": "split",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"web_accessible_resources": [
		"index.html"
	],
	"sandbox": {
		"pages": ["sandbox.html"]
	},
	"manifest_version": 2,
	"content_security_policy": "script-src 'self'; object-src 'self'"
}